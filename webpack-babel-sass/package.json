{
  "name": "webpack-babel-sass",
  "description": "Basic starting point for Webpack build with support for Babel and Sass",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run build && ./node_modules/.bin/serve dist",
    "dev": "node dev-server.js",
    "build": "rimraf dist && webpack --config ./webpack.production.js --progress --profile --colors"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "normalize.css": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.11.2",
    "postcss-loader": "^1.1.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "serve": "^1.4.0",
    "stats-webpack-plugin": "^0.4.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
