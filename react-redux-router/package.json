{
  "name": "react-redux-router",
  "description": "Setup with dependencies to create a React application with Redux, ReduxRouter and ImmutableJS.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/gulp develop",
    "build": "NODE_ENV=production ./node_modules/.bin/gulp build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-export-extensions",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.2.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "keymaster": "^1.6.2",
    "normalizr": "^2.1.0",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-router": "^2.0.0",
    "redux-thunk": "^2.1.0",
    "whatwg-fetch": "^1.0.0"
