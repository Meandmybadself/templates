    "plugins": [
      "transform-export-extensions",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "classnames": "^2.2.1",
    "connect-history-api-fallback": "^1.1.0",
    "history": "^1.13.1",
    "immutable": "^3.7.6",
    "keymaster": "^1.6.2",
    "normalizr": "^1.4.0",
    "react": "^0.14.5",
    "react-addons-css-transition-group": "^0.14.5",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.2",
    "react-router": "^1.0.2",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-router": "^1.0.0-beta5",
    "redux-thunk": "^1.0.2",
