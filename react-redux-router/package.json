{
  "name": "react-redux-router",
  "description": "Setup with dependencies to create a React application with Redux, ReduxRouter and ImmutableJS.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/gulp develop",
    "build": "NODE_ENV=production ./node_modules/.bin/gulp build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
